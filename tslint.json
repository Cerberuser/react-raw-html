{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "member-access": false,
        "member-ordering": false,
        "jsx-no-multiline-js": false,
        "semicolon": [true, "always"]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["dist"]
    }
}
